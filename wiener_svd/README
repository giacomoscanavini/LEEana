1. (in LEEana)               ./run_xs.pl
2. (in LEEana/wiener_svd)    ./copy.pl
3. (in LEEana/wiener_svd)    root -l --web=off convert_wiener_simple.C
4. (in LEEana/wiener_svd)    ../bin/wiener_example wiener.root output.root 2 0.5    // With wiener filter
5. (in LEEana/wiener_svd)    ../bin/wiener_example wiener.root output.root 2 0.5 0  // No-wiener filter (Ac matrix should be identity)