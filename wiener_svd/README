1. (in LEEana)               ./run_xs.pl
2. (in LEEana/wiener_svd)    ./copy.pl


3.a (in LEEana/wiener_svd)    root -l --web=off convert_wiener_2ch.C     			// FC0p, FCNp
3.b (in LEEana/wiener_svd)    root -l --web=off convert_wiener_4ch.C 				FC0p, FCNp, PC0p, PCNp

4.a (in LEEana/wiener_svd)    ../bin/wiener_example wiener.root output.root 2 0.5    // With wiener filter
A.b (in LEEana/wiener_svd)    ../bin/wiener_example wiener.root output.root 2 0.5 0  // No-wiener filter (Ac matrix should be identity)